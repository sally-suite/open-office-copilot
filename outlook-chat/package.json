  "license": "MIT",
  "config": {
    "app_to_debug": "outlook",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3001
  },
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production node ./script/build-cdn.mjs",
    "dev": "NODE_ENV=development webpack serve --mode development",
    "mock": "NODE_ENV=development MOCK=true webpack serve --config webpack.dev.config.js --mode development --name email-chat",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "start": "NODE_ENV=development office-addin-debugging start manifest.xml",
    "start:desktop": "NODE_ENV=development office-addin-debugging start manifest.xml desktop",
    "start:web": "NODE_ENV=development office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "NODE_ENV=development webpack --mode development --watch",
    "build-prod": "node ./script/build-prod.mjs",
    "testmail": "npm run start:web -- --document 'https://outlook.live.com/mail/0/'"
  },
